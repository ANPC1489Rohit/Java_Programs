// WAP to used the constructor 1:SI, 2:Sum of digit,3:Greater between 2no.

import java.util.Scanner;

public class Constructor 
{
	public static void main(String[] args)
	{
	
		Constructor obj=new Constructor();
		obj.Sum();
		obj.Greater();
		obj.SI();
	}
	
	
	public void Sum()
	{
		int a=45,b=67,sum;
		sum=a+b;
		
		System.out.println("the Sum of number is:" +sum);
		
	}
	
	public  void Greater()
	{
    
    int num1;
    int num2;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter your two number:");
		num1 =sc.nextInt();
		num2=sc.nextInt();
		
		if(num1>num2)
		{
			System.out.println("greater between 2 number is :"+num1);
		}
		else
		{
			System.out.println("greater between 2 number is :" +num2);
		}
		
	}
	
	public void  SI()
	{
		float P = 200, R = 35, T = 5;
		  
      
        float SI = (P * T * R) / 100;
        System.out.println("Simple interest = " + SI);
	}

}


