package blue_sky;

public class busService
{





	public void busService1() 
	{
		
		int bus_no=0;
		 busService obj[] = new  busService[4];
		
		obj[0] = new busService("Ratnagiri",40,"Mumbai to Ratnagiri",410);
		obj[1] = new busService("Mumbai",50,"Mumbai to Pune", 790);
		obj[2] = new busService("Pune", 45,"pune to Mumbai",540);
		obj[3] = new busService("Surat",55,"Mumbai to Surat",670);
	
	    for(int a=0;a<obj.length;a++)
		{
		
			 obj[a].display();
			System.out.println(" ");
		}
	   
	     
	    		
	}
	



	
	
}


